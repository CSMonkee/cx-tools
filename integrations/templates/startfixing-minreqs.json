{
  "github": {
    "autoDepUpgradeEnabled": true,
    "autoDepUpgradeIgnoredDependencies": [],
    "autoDepUpgradeLimit": 5,
    "autoPrivateDepUpgradeEnabled": false,
    "autoRemediationPrs": {
        "customRule": {
            "type": "severity",
            "score": null,
            "severityTypes": [
                "critical",
                "high"
            ]
        },
        "freshPrsEnabled": true,
        "backlogPrStrategy": "vuln",
        "backlogPrsEnabled": true,
        "usePatchRemediation": true
    },
    "isMajorUpgradeEnabled": false,
    "manualRemediationPrs": {
      "useManualPatchRemediation": false
    },
    "pullRequestAssignment": {
      "type": "auto",
      "enabled": true,
      "assignees": []
    },
    "pullRequestFailOnlyForHighSeverity": true,
    "pullRequestFailOnlyForIssuesWithFix": true,
    "pullRequestTestCodeEnabled": true,
    "pullRequestTestCodeSeverity": "high",
    "pullRequestTestEnabled": true,
    "reachableVulns": {
      "enabled": false
    }
  }
}
